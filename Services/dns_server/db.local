$TTL    604800
@       IN      SOA     ns1.example.com. admin.example.com. (
                          3         ; Serial
                     604800         ; Refresh
                      86400         ; Retry
                    2419200         ; Expire
                     604800 )       ; Negative Cache TTL

; Name servers
        IN      NS      ns1.example.com.

; IP addresses for name servers
ns1     IN      A       192.168.1.10

; Aliases
www     IN      CNAME   ns1
